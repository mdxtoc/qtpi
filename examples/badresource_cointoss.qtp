(* bad use of x in Q *)

proc P(fromQ:^qbit, toQ:^qbit) = | fromQ?(y) . toQ!y . _0
                                 | fromQ?(y) . y>>_X . toQ!y . _0
                     
proc Q(x: qbit, toP:^qbit, fromP:^qbit) = 
  x>>_H . toP!x . fromP?(z) . x>>_H . _0 

proc System() = (let bv = read_alternative "basis" "," ["0",|0>; "1",|1>; "+",|+>; "-",|->])
                (newq x = bv) 
                (new s:^qbit, r:^qbit) 
                | P(s,r) 
                | Q(x,s,r)
