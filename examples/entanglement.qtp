proc System() =
  (newq a = if randbit()=0b0 then |0> else |1> fi,
        b = if randbit()=0b0 then |0> else |1> fi)  
  out!["entangling "] . outq!(qval a) .
  out![" and "] . outq!(qval b) .
  a>>H . a,b >> CNot . 
  out![" gives "] . outq!(qval a) . out!["\n"] .
  | Alice (a) | Bob (b)
  
proc Alice (a) =
  a -/- (x) . 
  out!["Alice sees "; show x; "\n"] .
  _0

proc Bob (b) =
  b -/- (y) . 
  out!["Bob sees "; show y; "\n"] .
  _0
