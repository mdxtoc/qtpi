proc P(s:^qubit) = 
  + s?(y) . if randbool() then y>>I .                      s!y .         
                          else y>>X . out!["(P turns); "]. s!y . 
                     
proc Q(q: qubit, s:^qubit) = 
  out!["initially "] . outq!(qval q) . out!["; "] .
  q>>H . s!q . s?(q') . q'>>H . 
  out!["finally "] . outq!(qval q') . out!["\n"] .
   

proc System() = (newq q = |0>) (new s:^qubit)
                | P(s) 
                | Q(q,s) 
