fun tp (a:gate) (b:gate) =
  [show a; "⊗"; show b; "="; show (a⊗b); "\n"]
  
proc System () =
  out!tp I Cnot .
  out!tp I Cnot .
  out!tp Cnot I .
  out!tp Cnot X .
  out!tp X Cnot .
  out!tp (Rx (𝝅/8)) (Rx (𝝅/8))  .
  out!tp (Ry (𝝅/8)) (Ry (𝝅/8))  .
  out![show (Ry (𝝅/8)*Ry (𝝅/8)); "; "; show (Ry (𝝅/8)*Ry (𝝅/8)*Ry (𝝅/8)); "\n"] .
  out!tp (Rz (𝝅/8)) (Rz (𝝅/8))  .
  
