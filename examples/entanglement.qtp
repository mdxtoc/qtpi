proc System() =
  (newq a,b) a=?(_) . b=?(_) . 
  {print_string "entangling "} . {print_qbit a} .
  {print_string " and "} . {print_qbit b} .
  a>>_H . a,b >> _Cnot . 
  {print_string " gives "} . {print_qbit a} . {print_string "\n"} .
  | Alice (a) | Bob (b)
  
proc Alice (a) =
  a =? (x) . 
  {print_strings ["Alice sees "; show x; "\n"]} .
  _0

proc Bob (b) =
  b =? (y) . 
  {print_strings ["Bob sees "; show y; "\n"]} .
  _0
