proc System() =
  (newq a,b) a=?(_) . b=?(_) . 
  out!["entangling "] . outq!(qval a) .
  out![" and "] . outq!(qval b) .
  a>>_H . a,b >> _Cnot . 
  out![" gives "] . outq!(qval a) . out!["\n"] .
  | Alice (a) | Bob (b)
  
proc Alice (a) =
  a =? (x) . 
  out!["Alice sees "; show x; "\n"] .
  _0

proc Bob (b) =
  b =? (y) . 
  out!["Bob sees "; show y; "\n"] .
  _0
